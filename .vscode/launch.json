  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch day x",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/2023/main.go",
      "args": [
        "2"
      ]
    },
    {
      "name": "Launch playground",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/2023/main.go",
      "args": [
        "p"
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "args": [
        "--day=3", 
       "--part=1"
      ],
      "cwd": "${workspaceFolder:AOC}/2023/swift",
      "name": "Debug AOC",
      "program": "${workspaceFolder:AOC}/2023/swift/.build/debug/aoc",
      "preLaunchTask": "swift: Build Debug AOC"
    },
    {
      "type": "lldb",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder}/2023/swift",
      "name": "Release AOC",
      "program": "${workspaceFolder}/2023/swift/.build/release/aoc",
      "preLaunchTask": "swift: Build Release AOC"
    }
  ]
}
